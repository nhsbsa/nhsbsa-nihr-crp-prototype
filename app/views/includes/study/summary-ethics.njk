{% set s = data.ethics or {} %}
<dl class="nhsuk-summary-list">
  <div class="nhsuk-summary-list__row">
    <dt class="nhsuk-summary-list__key">REC approval</dt>
    <dd class="nhsuk-summary-list__value">{{ s.recApproval or '—' }}</dd>
  </div>
  <div class="nhsuk-summary-list__row">
    <dt class="nhsuk-summary-list__key">REC reference</dt>
    <dd class="nhsuk-summary-list__value">{{ s.recRef or '—' }}</dd>
  </div>
</dl>
